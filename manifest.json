{
    "manifest_version": 2,
    "name": "PopUp Dictionary",
    "version": "2.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background":{
        "scripts":["background.js"]

    },
    "browser_action": {
        "default_icon": "ct1.png",
        "default_popup":"popup.html",
        "default_title": "Here is the Definition"
    }
}